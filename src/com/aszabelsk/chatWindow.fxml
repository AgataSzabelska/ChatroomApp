                    </children>
                </HBox>
                <ListView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
                <ListView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                <HBox>
                    <children>
                        <TextField HBox.hgrow="ALWAYS"/>
                        <Button mnemonicParsing="false" text="Send"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</HBox>
